= form_for [@artist,@album], :html => { :multipart => true } do |form|
  .span-20.last
    = form.error_messages
  .span-4  
    = form.label :title
  .span-16.last
    = form.text_field :title
  .span-4
    = form.label :cover
  .span-16.last    
    = form.file_field :cover
  .span-20.last.action
    = form.submit
